{
	"manifest_version": 2,
	"name": "Mine Extension",
	"version": "1.0",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],

  "browser_action": {
    "default_popup" : "index.html",
		"default_icon" : "1.png"
  },
  "permissions": [
  	"tabs"
  ]
}
